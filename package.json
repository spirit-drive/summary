    "fast-deep-equal": "^3.1.3",
    "i18next": "^20.3.4",
    "query-string": "^7.0.1",
    "random-js": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.11.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "unchanged": "^2.2.1",
    "unfetch": "^4.2.0"
  }
}
